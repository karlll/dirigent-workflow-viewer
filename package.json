    "url": "https://github.com/karlll/dirigent-workflow-viewer/issues"
  },
  "keywords":  [
    "react",
    "workflow",
    "visualization",
    "dirigent",
    "react-flow"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
