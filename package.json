{
  "name": "@dirigent/workflow-viewer",
  "private": false,
  "version": "0.1.0",
  "type": "module",
  "description": "React component for visualizing Dirigent workflow YAML files",
  "author": "Karl Larsaeus",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/karlll/dirigent-workflow-viewer. git"
  },
  "homepage": "https://github.com/karlll/dirigent-workflow-viewer#readme",
